{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "ES6"],
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "allowJs": true
  },
  "exclude": ["**/node_modules", "**/coverage", "**/dist", "**/build"]
}
